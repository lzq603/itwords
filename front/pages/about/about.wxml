<view class="header container">
  <image class="header-logo" mode="aspectFill" src="../../images/logo.png"></image>
  <view class="header-title">{{ title }}</view>
  <view class="header-version">{{ version }}</view>
</view>
<view class="footer">
  <view class="footer-copyright">{{ copyright }}</view>
  <view class="footer-meta">{{ meta.origin }}</view>
  <view class="footer-meta">{{ meta.opensource }}</view>
</view>
